<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:practicas="http://www.unijob.edu/practicas#">

  <!-- Ontología base -->
  <owl:Ontology rdf:about="http://www.unijob.edu/practicas#">
    <rdfs:comment>
      Ontología para un sistema de recomendación de prácticas profesionales.
      Modela estudiantes, competencias, carreras, empresas y oportunidades laborales.
    </rdfs:comment>
  </owl:Ontology>

  <!-- Clases principales -->
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Estudiante"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Carrera"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Competencia"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Empresa"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#OportunidadLaboral"/>

  <!-- Propiedades de datos -->
  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombre">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombreCarrera">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Carrera"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombreCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombreEmpresa">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Empresa"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#descripcionOportunidad">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OportunidadLaboral"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Propiedades de objeto (relaciones) -->
  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#perteneceACarrera">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Carrera"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#poseeCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#requiereCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OportunidadLaboral"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#ofrecidaPor">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OportunidadLaboral"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Empresa"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#recomendarA">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OportunidadLaboral"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
  </owl:ObjectProperty>

</rdf:RDF>
