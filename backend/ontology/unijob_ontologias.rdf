<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:practicas="http://www.unijob.edu/practicas#">

  <!-- Ontología base -->
  <owl:Ontology rdf:about="http://www.unijob.edu/practicas#">
    <rdfs:comment>
      Ontología para un sistema de recomendación de prácticas profesionales.
      Modela estudiantes, competencias, carreras, empresas y oportunidades laborales.
    </rdfs:comment>
  </owl:Ontology>

  <!-- Clases según especificación del usuario -->
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Estudiante"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Carrera"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Competencia"/>
  
  <owl:Class rdf:about="http://www.unijob.edu/practicas#Empresa"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#OfertaPractica"/>
  <owl:Class rdf:about="http://www.unijob.edu/practicas#AgenteRecomendador"/>

  <!-- Propiedades de datos y sus dominios según especificación -->
  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombre">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Cédula / número de documento del estudiante (PII) -->
  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#cedula">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Número de documento de identidad del estudiante (guardar con precaución)</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#preferencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nivelAcademico">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#ubicacion">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombreCarrera">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Carrera"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#area">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Carrera"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombreCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nivelCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#categoria">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#titulo">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#descripcion">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#modalidad">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#nombreEmpresa">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Empresa"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#sector">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Empresa"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#ubicacionEmpresa">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Empresa"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- Propiedades de objeto (relaciones) según la especificación -->
  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#perteneceACarrera">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Carrera"/>
    <rdfs:comment>Asocia el estudiante con su carrera</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#poseeCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
    <rdfs:comment>Indica las competencias desarrolladas</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#ofreceCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Carrera"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#empresa">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Empresa"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#requiereCompetencia">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Competencia"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#ofreceOportunidad">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Empresa"/>
  <rdfs:range rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:comment>Conecta la empresa con sus ofertas</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#recomendarA">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#AgenteRecomendador"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:comment>Indica una recomendación hecha por el agente</rdfs:comment>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#recomienda">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#AgenteRecomendador"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:comment>Especifica la práctica recomendada</rdfs:comment>
  </owl:ObjectProperty>

  <!-- Propiedades para preferencias explícitas del usuario -->
  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#ubicacionPreferida">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Ubicación geográfica preferida para la búsqueda de prácticas (ej. ciudad)</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#modalidadPreferida">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Modalidad de trabajo preferida (presencial, remota, híbrida)</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#salarioPreferido">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Rango o cifra del salario preferido para una oferta (guardar como literal)</rdfs:comment>
  </owl:DatatypeProperty>
  <!-- Propiedades para feedback de interacciones/recomendaciones -->
  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#motivoInteraccion">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Texto libre indicando la razón por la cual el usuario rechazó o comentó una recomendación</rdfs:comment>
  </owl:DatatypeProperty>

  <owl:ObjectProperty rdf:about="http://www.unijob.edu/practicas#reaccionSobre">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.unijob.edu/practicas#OfertaPractica"/>
    <rdfs:comment>Relaciona la reacción/feedback del estudiante hacia una oferta específica</rdfs:comment>
  </owl:ObjectProperty>

  <!-- Propiedad para almacenar password hasheada -->
  <owl:DatatypeProperty rdf:about="http://www.unijob.edu/practicas#contrasenaHash">
    <rdfs:domain rdf:resource="http://www.unijob.edu/practicas#Estudiante"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Hash de contraseña (scrypt) almacenado como salt$hash en hexadecimal</rdfs:comment>
  </owl:DatatypeProperty>

</rdf:RDF>
